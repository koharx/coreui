{
  "name": "@koharx/core-ui",
  "version": "1.0.0",
  "description": "A React library with authentication, API calling, logging, and alert functionality",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "dev": "rollup -c -w",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run test"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "jwt-decode": "^4.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "rollup": "^3.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tslib": "^2.6.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "react",
    "authentication",
    "api",
    "logging",
    "alerts",
    "typescript",
    "library"
  ],
  "author": "KH Universe",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khuniverse/core-ui.git"
  },
  "bugs": {
    "url": "https://github.com/khuniverse/core-ui/issues"
  },
  "homepage": "https://github.com/khuniverse/core-ui#readme"
}
